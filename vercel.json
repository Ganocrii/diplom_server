{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/routes/products.js"
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "server.js"
      }
    ]
  }
  